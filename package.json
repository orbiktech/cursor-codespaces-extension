{
  "name": "cursor-codespaces",
  "displayName": "Cursor Codespaces",
  "description": "Seamless Codespaces connectivity via GitHub CLI",
  "version": "0.1.0",
  "publisher": "orbiktech",
  "repository": {
    "type": "git",
    "url": "https://github.com/orbiktech/cursor-codespaces-extension.git"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "icon": "assets/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "cursorCodespaces.connect",
        "title": "Connect to Codespace",
        "category": "Cursor Codespaces"
      },
      {
        "command": "cursorCodespaces.connectToCodespaceFromExplorer",
        "title": "Connect",
        "icon": "$(remote)"
      },
      {
        "command": "cursorCodespaces.refreshExplorer",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codespaces",
          "title": "GitHub Codespaces",
          "icon": "$(remote)"
        }
      ]
    },
    "views": {
      "codespaces": [
        {
          "id": "codespacesExplorer",
          "name": "Codespaces",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "cursorCodespaces.refreshExplorer",
          "when": "view == codespacesExplorer",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "cursorCodespaces.connectToCodespaceFromExplorer",
          "when": "view == codespacesExplorer && viewItem == codespace",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "ts-loader": "^9.5.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
